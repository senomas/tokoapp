{
  "name": "tokoapp",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "docker run --rm -v `pwd`/nginx.conf:/etc/nginx/nginx.conf:ro -v `pwd`/build:/data/www -p 3000:3000 nginx",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "supabase": "cd ../supabase && docker-compose up -d",
    "supabase-stop": "cd ../supabase && docker-compose down -v",
    "flyway": "docker run --rm --network=tokoapp_default -v `pwd`/../sql:/flyway/sql -v `pwd`/../flyway.conf:/flyway/conf/flyway.conf flyway/flyway",
    "init-data": "npm run flyway -- -target=2 migrate && node init-user.js && npm run flyway migrate",
    "predev": "npm run supabase && npm run init-data && npm run flyway migrate",
    "postdev": "npm run supabase-stop"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.28",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "~4.5.4"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@supabase/supabase-js": "^1.30.7",
    "svelte-fa": "^2.4.0"
  }
}
